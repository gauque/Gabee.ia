{

    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "build": {
        
          "api/**/main.py": {
            "excludeFiles": "public/**, testdata/**, out/**, etc/**"
          },
          
          "src":"api/main.py",
          "use":"@vercel/python"
          
        }
      
}
